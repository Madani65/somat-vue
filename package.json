    "vue-apexcharts": "^1.6.0",
    "vue-api-query": "^1.9.0",
    "vue-easytable": "^2.5.1",
    "vue-filter-date-format": "^1.3.1",
    "vue-moment": "^4.1.0",
    "vue-nestable": "^2.6.0",
    "vue-router": "^3.5.1",
    "vue-sweetalert2": "^4.2.0",
    "vue-table-dynamic": "^0.4.4",
    "vue-toastr": "^2.1.2",
